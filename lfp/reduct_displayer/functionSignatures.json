            {"name": "umap_min_dist", "kind": "namevalue", "type": "double"},
            {"name": "umap_init", "kind": "namevalue", "type": ["char", "choices={'spectral','random'}"]},
            {"name": "umap_fast_approx", "kind": "namevalue", "type": ["char", "choices={'True','False'}"]},
            {"name": "cluster_method", "kind": "namevalue", "type": ["char", "choices={'Manual','by Label','kmeans','dbscan','hierarchical','None'}"]},
            {"name": "cluster_source", "kind": "namevalue", "type": ["char", "choices={'Reduced Data 2d','Distance Mat','Base Waveforms'}"]},
            {"name": "cluster_dist_fun", "kind": "namevalue", "type": "char"},
            {"name": "cluster_wv_win", "kind": "namevalue", "type": "double"},
            {"name": "cluster_all_norm", "kind": "namevalue", "type": "logical"},
            {"name": "cluster_z_norm", "kind": "namevalue", "type": "logical"},
            {"name": "cluster_align_norm", "kind": "namevalue", "type": "logical"},
            {"name": "kmean_nClusters", "kind": "namevalue", "type": "double"},
            {"name": "kmean_dist_fun", "kind": "namevalue", "type": ["char", "choices={'sqeuclidean','cityblock','cosine','correlation','hamming'}"]},
            {"name": "kmean_start", "kind": "namevalue", "type": ["char", "choices={'plus','cluster','sample','uniform'}"]},
            {"name": "kmean_MaxIter", "kind": "namevalue", "type": "double"},
            {"name": "kmean_online", "kind": "namevalue", "type": ["char", "choices={'On','Off'}"]},
            {"name": "kmean_replicates", "kind": "namevalue", "type": "double"},
            {"name": "dbscan_epsilon", "kind": "namevalue", "type": "double"},
            {"name": "dbscan_minpts", "kind": "namevalue", "type": "double"},
            {"name": "dbscan_dist_fun", "kind": "namevalue", "type": [["char"], ["choices={'precomputed', 'euclidean', 'squaredeuclidean', 'seuclidean', 'mahalanobis', 'cityblock', 'minkowski', 'chebychev', 'cosine', 'correlation', 'hamming', 'jaccard', 'spearman'}"]]},
            {"name": "HC_link_method", "kind": "namevalue", "type": ["char", "choices={'weighted','average','centroid','complete','median','single','ward'}"]},
            {"name": "HC_clust_critera", "kind": "namevalue", "type": ["char", "choices={'inconsistent','distance'}"]},
            {"name": "HC_clust_cutoff", "kind": "namevalue", "type": "double"},
            {"name": "HC_clust_MaxClust", "kind": "namevalue", "type": "double"},
            {"name": "disp_wv_win", "kind": "namevalue", "type": "double"},
            {"name": "disp_all_norm", "kind": "namevalue", "type": "logical"},
            {"name": "disp_z_norm", "kind": "namevalue", "type": "logical"},
            {"name": "disp_align_norm", "kind": "namevalue", "type": "logical"},
            {"name": "disp_type", "kind": "namevalue", "type": ["char", "choices={'Full Cluster Average','Clusters as Labels','Overlays'}"]}
