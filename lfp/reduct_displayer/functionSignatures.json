{
    "reduct_displayer": {
        "inputs": [
            {"name": "ied_obj", "kind": "required", "type": "IED.data"},
            {"name": "wave_labels", "kind": "ordered", "type": "cell"},
            {"name": "reduction_function", "kind": "namevalue", "type": ["char", "choices={'PCA','t-SNE','UMAP','cMDS'}"]},
            {"name": "reduction_source", "kind": "namevalue", "type": ["char", "choices={'Base Waveforms','Distance Mat'}"]},
            {"name": "reduction_dist_fun", "kind": "namevalue", "type": "char"},
            {"name": "reduction_wv_win", "kind": "namevalue", "type": "double"},
            {"name": "reduction_all_norm", "kind": "namevalue", "type": "logical"},
            {"name": "reduction_z_norm", "kind": "namevalue", "type": "logical"},
            {"name": "reduction_align_norm", "kind": "namevalue", "type": "logical"},
            {"name": "tsne_dist_fun", "kind": "namevalue", "type": [["char"], ["choices={'euclidean', 'seuclidean', 'fasteuclidean', 'fastseuclidean', 'cityblock', 'chebychev', 'minkowski', 'mahalanobis', 'cosine', 'correlation', 'spearman', 'hamming', 'jaccard'}"]]},
            {"name": "tsne_exaggeration", "kind": "namevalue", "type": "double"},
            {"name": "tsne_perplexity", "kind": "namevalue", "type": "double"},
            {"name": "tsne_Algorithm", "kind": "namevalue", "type": ["char", "choices={'exact','barneshut'}"]},
            {"name": "tsne_LearnRate", "kind": "namevalue", "type": "double"},
            {"name": "tsne_max_iter", "kind": "namevalue", "type": "double"},
            {"name": "umap_dist_fun", "kind": "namevalue", "type": [["char"], ["choices={'euclidean', 'seuclidean', 'fasteuclidean', 'fastseuclidean', 'cityblock', 'chebychev', 'minkowski', 'mahalanobis', 'cosine', 'correlation', 'spearman', 'hamming', 'jaccard'}"]]},
            {"name": "umap_n_neighbors", "kind": "namevalue", "type": "double"},
